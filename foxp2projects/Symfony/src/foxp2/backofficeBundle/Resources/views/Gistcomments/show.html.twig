{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Gistcomments</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Idgist</th>
                <td>{{ entity.idGist }}</td>
            </tr>
            <tr>
                <th>Filenamegist</th>
                <td>{{ entity.filenameGist }}</td>
            </tr>
            <tr>
                <th>Comments</th>
                <td>{{ entity.comments }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('gistcomments_index') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('gistcomments_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
